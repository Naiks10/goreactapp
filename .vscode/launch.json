    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch via NPM",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "start",
                "debug"
            ],
            "port": 3000,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "ADDRESS" : "<Путь к базе>",
                "PORT" : 1012,
            },
            "args": []
        }
    ]
}